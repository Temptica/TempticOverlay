; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Temptic404Overlay"
config/tags=PackedStringArray("c#", "overlay")
run/main_scene="res://overlay.tscn"
config/features=PackedStringArray("4.4", "C#", "Forward Plus")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://audio_bus_layout.tres"
general/text_to_speech=true
driver/output_latency=1
driver/enable_input=true
driver/mix_rate=48000

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/size/mode=3
window/size/initial_position_type=3
window/size/initial_screen=1
window/size/transparent=true
window/stretch/mode="viewport"
window/per_pixel_transparency/allowed=true

[dotnet]

project/assembly_name="Temptica.Overlay"

[editor_plugins]

enabled=PackedStringArray("res://addons/kanban_tasks/plugin.cfg")

[layer_names]

3d_render/layer_1="Glowables"
3d_physics/layer_1="walls"
3d_physics/layer_2="Fish"
3d_physics/layer_3="Bubble"
3d_physics/layer_4="Floor"
3d_physics/layer_5="Peepo"
3d_physics/layer_6="Plinko"
3d_physics/layer_7="BattleFish"
3d_physics/layer_8="SnowFlake"
3d_physics/layer_9="SnowBall"
3d_physics/layer_10="Otter"
3d_physics/layer_11="Plushie"
3d_physics/layer_12="Pipe"
3d_physics/layer_13="Trash"
3d_physics/layer_14="DraggableArea"

[rendering]

textures/canvas_textures/default_texture_filter=0
anti_aliasing/quality/msaa_3d=3
anti_aliasing/screen_space_roughness_limiter/amount=4.0
anti_aliasing/screen_space_roughness_limiter/limit=1.0
viewport/transparent_background=true
